        <item name="android:textSize">@dimen/text_12dp</item>
    </style>

    <style name="civ_home">
        <item name="android:layout_width">@dimen/width_72dp</item>
        <item name="android:layout_height">@dimen/width_72dp</item>
        <item name="android:padding">14dp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="rl_home">

<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="tv_title" parent="match_wrap">
        <item name="android:background">@color/bg_gray</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/padding_10dp</item>
    </style>

    <style name="btn_style" parent="match_wrap">
        <item name="android:gravity">center</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/color_DarkGray</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/shape_tv_button</item>
    </style>

    <style name="tv_profile" parent="wrap_wrap">
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="iv_signature">
        <item name="android:layout_height">@dimen/iv_24dp</item>
        <item name="android:layout_width">@dimen/iv_24dp</item>

    </style>

    <style name="tv_profile_text" parent="match_wrap">
        <item name="android:gravity">center</item>
        <item name="android:layout_marginBottom">@dimen/margin_10dp</item>
        <item name="android:textColor">@color/color_DarkGray</item>
    </style>

    <style name="iv_row">
        <item name="android:layout_width">@dimen/iv_32dp</item>
        <item name="android:layout_height">@dimen/iv_32dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="logo_notification">
        <item name="android:layout_height">@dimen/iv_32dp</item>
        <item name="android:layout_width">@dimen/iv_32dp</item>
    </style>

    <style name="lay_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_50dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="lay_foot">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="iv_footer">
        <item name="android:layout_width">@dimen/iv_24dp</item>
        <item name="android:layout_height">@dimen/iv_24dp</item>
    </style>

    <style name="tv_profile_heading" parent="wrap_wrap">
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_14dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_gravity">center</item>
    </style>


    <style name="login_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/padding_4dp</item>

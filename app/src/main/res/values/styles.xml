    </style>

    <style name="eveation_text" parent="wrap_wrap">
        <item name="android:textColor">@color/dark_gray</item>
    </style>

    <style name="eveationdetail" parent="match_wrap">

    </style>

    <style name="eveation_link" parent="wrap_wrap">
        <item name="android:textColor">@color/color_blue_bg</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="ll_membership" parent="match_wrap">
        <item name="android:orientation">vertical</item>
    </style>

